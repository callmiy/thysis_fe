{
  "name": "thysis",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.1.0",
    "apollo-boost": "^0.3.1",
    "apollo-cache-persist": "^0.1.1",
    "apollo-link-state": "^0.4.2",
    "date-fns": "^2.0.0-alpha.27",
    "formik": "1.5.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "immutability-helper": "^3.0.0",
    "jss": "^9.8.7",
    "jss-preset-default": "^4.5.0",
    "lodash-es": "^4.17.11",
    "phoenix": "^1.4.1",
    "polished": "^3.0.2",
    "react": "^16.8.3",
    "react-apollo": "^2.5.1",
    "react-dom": "^16.8.3",
    "react-jss": "^8.4.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "^2.1.5",
    "react-select": "^1.2.1",
    "react-transition-group": "^2.6.0",
    "semantic-ui-css-offline": "^1.0.4-2.4.2",
    "semantic-ui-react": "^0.85.0",
    "styled-components": "^4.1.3",
    "tslib": "1.9.3",
    "yup": "^0.26.10"
  },
  "scripts": {
    "start": "cross-env PORT=3016 BROWSER=none REACT_APP_API_URL=http://localhost:4016/api yarn react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env REACT_APP_API_URL=http://localhost:4016/api react-scripts test --env=jsdom",
    "deploy-prod": "yarn deploy-netlify && yarn deploy-heroku",
    "deploy-netlify": "cross-env REACT_APP_API_URL=https://thysis.herokuapp.com/api react-scripts build && netlify deploy  --prod --dir=build",
    "deploy-heroku": "cd .. && git co master && git merge dev && git subtree push --prefix=server heroku master && git co dev && cd client",
    "gen-apollo-types": "apollo codegen:generate --endpoint=http://localhost:4016/api --tagName=gql --target=typescript --includes=src/graphql/*.ts --outputFlat=src/graphql/apollo-types",
    "build-local-prod": "cross-env REACT_APP_API_URL=http://localhost:4017/api  react-scripts build",
    "start-local-prod": "serve --no-clipboard --single --listen 3017 build",
    "cy-open": "cypress open",
    "cy-run": "cypress run",
    "test-e2e-prod-run": "yarn build-local-prod && start-server-and-test start-local-prod http://localhost:3017 cy-run",
    "test-e2e-prod-open": "yarn build-local-prod && start-server-and-test start-local-prod http://localhost:3017 cy-open",
    "test-e2e-prod-run-no-bild": "start-server-and-test start-local-prod http://localhost:3017 cy-run",
    "test-e2e-prod-open-no-build": "start-server-and-test start-local-prod http://localhost:3017 cy-open",
    "test-e2e-dev-run": "start-server-and-test start http://localhost:3016 cy-run"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.9",
    "@types/jss": "^9.5.3",
    "@types/lodash-es": "^4.17.2",
    "@types/node": "10.12.7",
    "@types/phoenix": "^0.0.5",
    "@types/prop-types": "15.5.6",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^1.2.6",
    "@types/react-transition-group": "^2.0.16",
    "@types/styled-components": "^4.1.11",
    "@types/yup": "^0.26.11",
    "apollo": "^2.5.3",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.5",
    "cypress-testing-library": "^2.3.6",
    "jest-dom": "^3.1.2",
    "jest-styled-components": "^6.3.1",
    "node-sass": "^4.11.0",
    "react-testing-library": "^6.0.0",
    "serve": "^10.1.2",
    "start-server-and-test": "^1.7.11",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/pwd-recovery-token-request/index.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "html"
    ]
  }
}
